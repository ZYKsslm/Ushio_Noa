transform down:
    ease 0.2 yoffset 100
    pause 0.1
    ease 0.2 yoffset 0

transform left_down:
    xoffset -202
    yoffset -138
    easein 0.4 rotate -15
    easein 0.4 rotate 5
    easeout 0.3 rotate -15 yoffset 800
    ease 0.3 rotate 0 yoffset -138

transform right_down:
    xoffset -202
    yoffset -138
    easein 0.4 rotate 15
    easein 0.4 rotate -5
    easeout 0.3 rotate 15 yoffset 800
    ease 0.3 rotate 0 yoffset -138